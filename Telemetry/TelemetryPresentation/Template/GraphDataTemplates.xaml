<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:graphPanel="clr-namespace:SecondMonitor.Telemetry.TelemetryApplication.ViewModels.GraphPanel;assembly=TelemetryApplication"
                    xmlns:oxy="http://oxyplot.org/wpf">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../TelemetryPresentation.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <DataTemplate x:Key="DummyGraphDataTemplate" DataType="graphPanel:AbstractGraphViewModel">
        <Border BorderThickness="2" BorderBrush="{StaticResource DarkGrey01Brush}">
        <Grid MinWidth="100" MinHeight="200" Background="Black">
            <oxy:PlotView Model="{Binding PlotModel}" Background="Black" Foreground="{StaticResource Anthracite01Brush}"/>
            </Grid>
        </Border>
    </DataTemplate>
</ResourceDictionary>