<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
                    xmlns:graphPanel="clr-namespace:SecondMonitor.Telemetry.TelemetryApplication.ViewModels.GraphPanel;assembly=TelemetryApplication">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../TelemetryPresentation.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <DataTemplate x:Key="DummyGraphDataTemplate" DataType="graphPanel:AbstractGraphViewModel">
        <Border BorderThickness="2" BorderBrush="{StaticResource DarkGrey01Brush}">
        <Grid MinWidth="100" MinHeight="200">
            <wpf:CartesianChart Series="{Binding SeriesCollection}" LegendLocation="None" Background="Black" Zoom="Xy" DisableAnimations="True" DataTooltip="{x:Null}">
                <wpf:CartesianChart.AxisY>
                    <wpf:Axis LabelFormatter="{Binding YFormatter}"/>
                </wpf:CartesianChart.AxisY>
                <wpf:CartesianChart.AxisX >
                    <wpf:Axis LabelFormatter ="{Binding XFormatter}"/>
                </wpf:CartesianChart.AxisX>
            </wpf:CartesianChart>
        </Grid>
        </Border>
    </DataTemplate>
</ResourceDictionary>